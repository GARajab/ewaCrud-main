    <div class="container" style="margin-top: -80px;">

    <h1 style="text-align: center"></h1>

    <h1 style="display: flex; justify-content: center; margin-top: 150px;">Add New Scheme</h1>

    <div class="row">
      <div class="col-md-5 mx-auto">
        <div id="first">

          <form action="/schemes/create" method="POST" name="addNewService">
            <!-- this is the localhost:3000 path without any thing will transfer you to the form action -->
             <br>
            <div>
              <label for="Sch_Ref">Reference Number</label>
              <input type="text" name="Sch_Ref" class="form-control" id="Sch_Ref"
                placeholder="Enter Your New Reference Number">
            </div>
            <br>
            <div class="form-group">
              <label for="APPNUMBER">Application Number</label>
              <input type="text" name="APPNUMBER" class="form-control" id="APPNUMBER"
                placeholder="Enter The Application Number">
            </div>
            <br>
            <div class="form-group">
              <label for="BLK">Block</label>
              <input type="text" name="BLK" class="form-control" id="BLK" 
                placeholder="Enter Block Number">
            </div>
            <br>
            <div class="form-group">
              <label for="SUPERVISOR">SUPER VISOR</label>
              <input type="text" name="SUPERVISOR" class="form-control" id="SUPERVISOR" 
                placeholder="Enter The SUPER VISOR">
            </div>
            <br>
            <div class="form-group">
              <label for="ContractorName">Contractor Name</label>
              <input type="text" name="ContractorName" class="form-control" id="ContractorName" 
                placeholder="Enter The Contractor Name">
            </div>
            <br>
            <div class="form-group">
              <label for="Title1">Title</label>
              <input type="text" name="Title1" class="form-control" id="Title1" 
                placeholder="Enter The Title">
            </div>
            <br>
            <div class="form-group">
              <label for="STATUS">STATUS</label>
              <input type="text" name="STATUS" class="form-control" id="STATUS" 
                placeholder="Enter The Status">
            </div>
            <br>
            <div class="form-group">
              <label for="PERCENTAGE">PERCENTAGE</label>
              <input type="text" name="PERCENTAGE" class="form-control" id="PERCENTAGE" 
                placeholder="Enter The PERCENTAGE">
            </div>
            <br>
            <div class="form-group">
              <label for="DATE_OF_COMPLETE">DATE OF COMPLETE</label>
              <input type="date" name="DATE_OF_COMPLETE" class="form-control" id="DATE_OF_COMPLETE" 
                placeholder="Enter The DATE OF COMPLETE">
            <br>
            <div class="form-group">
              <label for="NUMBEROFSS">NUMBER OF Substations</label>
              <input type="text" name="NUMBEROFSS" class="form-control" id="NUMBEROFSS" 
                placeholder="Enter The Number Of Substations">
            </div>
            <br>
            <div class="form-group">
              <label for="APPSTATUS">APP STATUS</label>
              <input type="text" name="APPSTATUS" class="form-control" id="APPSTATUS" 
                placeholder="Enter The App Status">
            </div>
            <br>
            <div class="form-group">
              <label for="DATEOFMEASUREMENT">DATE OF MEASUREMENT</label>
              <input type="date" name="DATEOFMEASUREMENT" class="form-control" id="DATEOFMEASUREMENT" 
                placeholder="Enter The Date Of Measurment">
            </div>
            <br>
            <div class="form-group">
              <label for="CONTRACTORAPPRAISAL">CONTRACTOR APPRAISAL</label>
              <input type="text" name="CONTRACTORAPPRAISAL" class="form-control" id="CONTRACTORAPPRAISAL" 
                placeholder="Enter The Contractor Appraisal">
            </div>
            <br>
            <div class="form-group">
              <label for="AREA">AREA</label>
              <input type="text" name="AREA" class="form-control" id="AREA" 
                placeholder="Enter The Area">
            </div>
            <br>
            <div class="form-group">
              <label for="TYPE">TYPE</label>
              <input type="text" name="TYPE" class="form-control" id="TYPE" 
                placeholder="Enter The Type">
            </div>
            <br>
            <div class="col-md-14 text-center">
              <button type="submit" style="font-size: 25px;" class=" btn btn-block mybtn btn-primary tx-tfm">Add</button>
            </div>
        </div>
        </form>
      </div>
    </div>
  </div>
